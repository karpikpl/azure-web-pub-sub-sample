{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "environmentName": {
      "value": "${AZURE_ENV_NAME}"
    },
    "location": {
      "value": "${AZURE_LOCATION}"
    },
    "principalId": {
      "value": "${AZURE_PRINCIPAL_ID}"
    },
    "solverImageName": {
      "value": "${SERVICE_SOLVER_IMAGE_NAME}"
    },
    "wpsServerImageName": {
      "value": "${SERVICE_WPS_SERVER_IMAGE_NAME}"
    },
    "solverAppExists": {
      "value": "${SERVICE_SOLVER_RESOURCE_EXISTS}"
    },
    "wpsServerAppExists": {
      "value": "${SERVICE_WPS_SERVER_RESOURCE_EXISTS}"
    },
    "doRoleAssignments": {
      "value": "${DO_ROLE_ASSIGNMENTS}"
    }
  }
}